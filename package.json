{
    "name": "document_manager",
    "version": "1.0.0",
    "description": "Document Manager App",
    "main": "server/server.js",
    "scripts": {
        "preinstall": "node client/tools/nodeVersionCheck.js",
        "setup": "node client/tools/setup/setupMessage.js && npm install && node client/tools/setup/setup.js",
        "pretest": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate && NODE_ENV=test sequelize db:seed:all",
        "prestart": "npm-run-all --parallel start-message",
        "start": "babel-node client/tools/srcServer.js",
        "start-message": "babel-node client/tools/startMessage.js",
        "open:src": "babel-node client/tools/srcServer.js",
        "open:dist": "babel-node client/tools/distServer.js",
        "test": "gulp production && NODE_ENV=test node node_modules/gulp/bin/gulp coverage  && gulp coveralls && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "start:production": "gulp production && NODE_ENV=production node build/server.js",
        "dev": "babel-node server/server.js",
        "start:dev": "nodemon --exec babel-node server/server.js",
        "build": "babel-node client/tools/build.js && npm run open:dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andela-Taiwo/Document_Manager.git"
    },
    "keywords": [
        "api",
        "sequelize",
        "mongoose",
        "nodejs"
    ],
    "author": "SOKUNBI TAIWO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/andela-Taiwo/Document_Manager/issues"
    },
    "homepage": "https://github.com/andela-Taiwo/Document_Manager#readme",
